{
  "kind": "collectionType",
  "collectionName": "eco_points",
  "info": {
    "singularName": "eco-point",
    "pluralName": "eco-points",
    "displayName": "EcoPoint",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "midia": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "name": {
      "type": "string",
      "required": true,
      "minLength": 3
    },
    "businessHours": {
      "type": "time",
      "required": true
    },
    "link": {
      "type": "string",
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    },
    "approved": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "adress": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "zipCode": {
      "type": "string",
      "required": true
    },
    "complement": {
      "type": "string"
    },
    "neighborhood": {
      "type": "string",
      "required": true
    },
    "number": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "ratings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rating.rating"
    }
  }
}
